// Project Euler Problem 3: 
// - Find the largest prime factor of 600851475143
main := () -> void {
    int num := 600851475143;

    int i := 2;

    int ans := num;
    while (!(i * i > num)) {
        bool flag := true;
        while (flag) {
            int div := num / i;
            if (!(div * i == num)) {
                flag := false;
            } else {
                num := num / i;
            }
        }
        i := i + 1;
    }

    println(num);
}
