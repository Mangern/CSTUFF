// Project Euler Problem 3: 
// - Find the largest prime factor of 600851475143
main := () -> void {
    int num := 600851475143;

    int i := 2;

    int ans := num;
    while (i * i <= num) {
        while (num % i == 0) {
            if (i == num) {
                break;
            }
            num /= i;
        }
        i += 1;
    }

    println(num);
}
