// Project Euler Problem 3: 
// - Find the largest prime factor of 600851475143
main: () -> void = {
    num := 600851475143;

    i := 2;
    while (i * i <= num) {
        while (num % i == 0) {
            if (i == num) {
                break;
            }
            num /= i;
        }
        i += 1;
    }

    println(num); // 6857
}
